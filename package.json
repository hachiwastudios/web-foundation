{
  "name": "@hachiwastudios/web-foundation",
  "version": "1.5.0",
  "description": "Professional HTML + CSS + JavaScript starter for rapid web development by Hachi wa Studios",
  "main": "index.js",
  "bin": {
    "hachiwa-web": "./bin/cli.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "node scripts/build.js",
    "build:css": "node scripts/build-css.js",
    "build:js": "node scripts/build-js.js",
    "build:watch": "npm-run-all --parallel watch:*",

    "watch": "npm-run-all --parallel watch:*",
    "watch:css": "chokidar 'src/css/**/*.css' -c 'npm run build:css'",
    "watch:js": "chokidar 'src/js/**/*.js' -c 'npm run build:js'",

    "dev": "npm-run-all build --parallel dev:*",
    "dev:watch": "npm run watch",
    "dev:serve": "live-server examples --port=3000",

    "start": "npm run dev",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "chokidar-cli": "^3.0.0",
    "clean-css-cli": "^5.6.3",
    "cssnano": "^7.1.2",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "rimraf": "^6.1.2"
  },
  "keywords": [
    "html",
    "css",
    "javascript",
    "starter",
    "template",
    "boilerplate",
    "utility-css",
    "framework",
    "web-development",
    "hachiwastudios",
    "themes",
    "responsive"
  ],
  "author": "Hachi wa Studios",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hachiwastudios/web-foundation"
  },
  "bugs": {
    "email": "hachiwastudios@gmail.com",
    "url": "https://hachiwastudios.netlify.app/"
  },
  "homepage": "https://hachiwastudios.netlify.app/",
  "files": [
    "bin/",
    "dist/",
    "docs/",
    ".vscode/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=12.0.0"
  }
}

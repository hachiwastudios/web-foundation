{
  "name": "web-foundation",
  "displayName": "Web Foundation by Hachi wa Studios",
  "description": "IntelliSense and autocomplete for Hachi wa Studios Web Foundation CSS utility classes and HTML elements",
  "version": "1.5.0",
  "publisher": "hachiwastudios",
  "repository": {
    "type": "git",
    "url": "https://github.com/hachiwastudios/web-foundation"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "keywords": [
    "html",
    "css",
    "utility-css",
    "tailwind",
    "autocomplete",
    "intellisense",
    "web-foundation",
    "hachiwa"
  ],
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:css",
    "onLanguage:javascript"
  ],
  "main": "./extension.js",
  "contributes": {
    "html": {
      "customData": [
        "./html-custom-data.json"
      ]
    },
    "css": {
      "customData": [
        "./css-custom-data.json"
      ]
    },
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/html.json"
      }
    ]
  },
  "scripts": {
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "vsce": "^2.15.0"
  }
}
